@using CarCatalog.Core.ViewModels.Transmision;
@model TransmisionViewModel


<h2 style="color: white">@ViewData["Title"]</h2>

<div class="align-content-center">

    <div class="row col-md-4 offset-4">
        <form method="post">
            <div class="form-group">
                <label asp-for="TransmisionType"></label>
                <input asp-for="TransmisionType" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Gears"></label>
                <input asp-for="Gears" class="form-control" />
            </div>
            <input class="btn text-black btn-info text-center " type="submit" value="Add" />
        </form>
    </div>
</div>


